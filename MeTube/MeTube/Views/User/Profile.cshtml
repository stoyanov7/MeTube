@model MeTube.Models.ApplicationUser

<div class="text-center">
    <h2 class="text-info text-center">Hello @Model.UserName!</h2>
    <h3 class="text-info text-center">@Model.Email</h3>
</div>
<div class="row">
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Title</th>
                <th>Author</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            @Model.Tubes
        </tbody>
    </table>
</div>